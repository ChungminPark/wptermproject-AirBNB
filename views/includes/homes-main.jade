form(action="/rooms/search", method='POST')
  .row
    .col-lg-12
      .input-group
        input(type="text", name='search', id='search', class="form-control" placeholder="city")
        span.input-group-btn
          button#search-home.btn.btn-primary(type='submit') 숙소 검색
  br
  h3 추천 숙소
  .row
    each room in rooms
      .col-sm-4.col-md-3
        .thumbnail
          a(href="/rooms/#{room.id}")
            img(src='data:image/png;base64,')
          .caption
            p ￦ #{room.fee} 
            p #{room.title}
            #roomtitle #{room.city}에 위치한 #{room.roomtype}
            br
            p
              span= room.reservationStatus
            p 
              a.btn.btn-primary(href="/rooms/#{room.id}" role="button") 상세 보기
              a.btn.btn-default(href="/books/#{room._id}" role="button") 예약 하기
